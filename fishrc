alias DF='echo cd $df; cd $df'
alias Ga='git add'
alias Gb='git branch'
alias Gc='git checkout'
alias GL='git log'
alias Gh='git push'
alias Gl='git pull'
alias Gs='git status'
alias b='echo cd -; cd -'
alias u='echo cd ..; cd ..'
alias ty='type'

function l
    echo ls -lhF $argv
    ls -lhF --color=always $argv | less
end

function s
    echo ls --color=always -CF $argv
    ls -CF --color=always $argv | less
end

function sf
    echo source $df/fishrc
    source $df/fishrc
end

