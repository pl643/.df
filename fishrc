alias a='alias'
alias b='echo cd -; cd -'
alias DF='echo cd $df; cd $df'
alias GA='grep git $df/fishrc'
alias Ga='git add'
alias Gb='git branch'
alias Gd='git diff'
alias Gcm='git commit -m'
alias Gco='git checkout'
alias GL='git log'
alias GH='git stash'
alias Gh='git push'
alias Gl='git pull'
alias Gs='git status'
alias GS='git switch'
alias le='less'
alias u='echo cd ..; cd ..'
alias ty='type'

function l
    echo ls -lhF $argv
    ls -lhF --color=always $argv | less
end

function s
    echo ls --color=always -CF $argv
    ls -CF --color=always $argv | less
end

function sf
    echo source $df/fishrc
    source $df/fishrc
end

